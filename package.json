{
  "name": "react-tsx",
  "version": "1.0.0",
  "description": "React Typescript i18n",
  "main": "index.ts",
  "engines": { "node": ">=16" },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.config.local.js --mode development",
    "build-dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.config.dev.js --mode development",
    "build-prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.prod.js --mode production",
    "test": "jest",
    "lint": "eslint src",
    "prettier:fix": "prettier --write src",
    "prettier:check": "prettier --check src",
    "test:watchAll": "jest --watchAll",
    "test:watch": "jest --watch --verbose --silent=false",
    "extract": "cross-env NODE_ENV=lingui lingui extract",
    "addlang": "cross-env NODE_ENV=lingui lingui add-locale",
    "extract:clean": "cross-env NODE_ENV=lingui lingui extract --clean",
    "compile": "cross-env NODE_ENV=lingui lingui compile",
    "compile:strict": "cross-env NODE_ENV=lingui lingui compile --strict"
  },
  "lint-staged": {
    "src/*.{ts,tsx,js,jsx}": [
      "npm run prettier:fix",
      "npm run lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ovidius72/react-tsx-i18n.git"
  },
  "keywords": [
    "react",
    "redux",
    "redux-toolkit",
    "typescript",
    "i18n",
    "lingui",
    "styled-components",
    "styled-system"
  ],
  "author": "Antonio Pantano",
  "license": "ISC",
  "homepage": "https://github.com/ovidius72/react-tsx-i18n.git",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime-corejs3": "^7.15.4",
    "@lingui/babel-preset-react": "^2.9.2",
    "@lingui/cli": "^3.12.1",
    "@lingui/loader": "^3.12.1",
    "@lingui/macro": "^3.12.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.4",
    "@types/react": "^17.0.29",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "5.1.15",
    "@types/styled-system": "^5.1.13",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^1.13.2",
    "clean-terminal-webpack-plugin": "^3.0.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "core-js": "3.18.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.4",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "mini-css-extract-plugin": "^2.4.2",
    "prettier": "^2.4.1",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "^17.0.2",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.4",
    "webpack": "5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "@lingui/react": "^3.12.1",
    "@reduxjs/toolkit": "^1.6.2",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.3.1",
    "favicons-webpack-plugin": "5.0.2",
    "history": "^5.0.1",
    "polished": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "regenerator-runtime": "^0.13.9",
    "styled-components": "5.3.1",
    "styled-icons": "^10.45.0",
    "styled-system": "^5.1.5"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/testing/test-shim.js",
      "<rootDir>/testing/test-setup.ts"
    ],
    "moduleNameMapper": {
      "^utils(.*)$": "<rootDir>/src/utils/$1",
      "^models(.*)$": "<rootDir>/src/models/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    },
    "testPathIgnorePatterns": [
      "/locale/",
      "/config/",
      "/styles/",
      "/typings/",
      "/coverage/",
      "/build/",
      "/.vscode/",
      "/.history/",
      "/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)",
      "**/*.spec.(ts|tsx|js)",
      "**/*.test.(ts|tsx|js)"
    ]
  },
  "_moduleAliases": {
    "models": "src/models",
    "utils": "src/utils",
    "images": "src/images/*",
    "components": "src/components/*",
    "src": "src"
  }
}

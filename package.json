{
  "name": "react-ts-mst",
  "version": "1.0.0",
  "description": "React Typescript Mobx State Tree",
  "main": "index.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js --mode development",
    "build-dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.config.dev.js --mode development",
    "build-prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.prod.js --mode production",
    "test": "jest",
    "test:watchAll": "jest -u --watchAll",
    "test:watch": "jest -u --watch --verbose --silent=false",
    "i18n:extract": "cross-env NODE_ENV=lingui lingui extract",
    "i18n:add": "cross-env NODE_ENV=lingui lingui add-locale",
    "i18n:extract:clean": "cross-env NODE_ENV=lingui lingui extract --clean",
    "i18n:compile": "cross-env NODE_ENV=lingui lingui compile",
    "i18n:compile:strict": "cross-env NODE_ENV=lingui lingui compile --strict"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ovidius72/react-tsx-i18n.git"
  },
  "keywords": ["react", "mobx state tree", "typescript", "i18n", "lingui", "styled-components"],
  "author": "Antonio Pantano",
  "license": "ISC",
  "homepage": "https://github.com/ovidius72/react-tsx-i18n.git",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@lingui/babel-preset-react": "^2.7.2",
    "@lingui/cli": "^2.7.2",
    "@lingui/loader": "^2.7.2",
    "@lingui/macro": "^2.7.2",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.9",
    "@types/lingui__core": "^2.2.0",
    "@types/lingui__react": "^2.5.0",
    "@types/mobx-devtools-mst": "^0.9.0",
    "@types/node": "^8.0.26",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.0",
    "@types/webpack": "^4.4.19",
    "@types/webpack-env": "^1.13.6",
    "@types/yup": "^0.26.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-macros": "^2.4.2",
    "babel-plugin-styled-components": "^1.8.0",
    "clean-terminal-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.15",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "include-media": "^1.4.9",
    "jest": "^23.6.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.4",
    "mobx-devtools-mst": "^0.9.18",
    "node-sass": "^4.10.0",
    "prettier": "^1.15.2",
    "react-test-renderer": "^16.6.3",
    "sass": "^1.15.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.6.0",
    "tslint-webpack-plugin": "^1.2.2",
    "typescript": "^3.1.6",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "webpack-messages": "^2.0.3",
    "webpackbar": "^3.1.3",
    "worker-loader": "^2.0.0",
    "workerize-loader": "^1.0.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@lingui/react": "^2.7.2",
    "classnames": "^2.2.6",
    "cuid": "^2.1.4",
    "favicons-webpack-plugin": "0.0.9",
    "mobx": "^5.6.0",
    "mobx-react": "^5.3.6",
    "mobx-state-tree": "^3.7.1",
    "mobx-utils": "^5.0.4",
    "polished": "^2.3.0",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-hot-loader": "^4.3.12",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.1.1",
    "styled-icons": "^4.0.0",
    "workerize": "^0.1.7",
    "wretch": "^1.4.2",
    "yup": "^0.26.6"
  },
  "lingui": {
    "fallbackLocale": "en",
    "sourceLocale": "en",
    "srcPathDirs": ["./src"],
    "srcPathIgnorePatterns": ["/node_modules/", "/public/", ".history", ".vscode"],
    "format": "lingui",
    "localeDir": "./locale"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    },
    "setupFiles": ["<rootDir>/test/test-shim.js", "<rootDir>/test/test-setup.js"],
    "moduleNameMapper": {
      "^utils(.*)$": "<rootDir>/src/utils/$1",
      "^models(.*)$": "<rootDir>/src/models/$1"
    },
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    },
    "testPathIgnorePatterns": [
      "/locale/",
      "/config/",
      "/styles/",
      "/typings/",
      "/coverage/",
      "/build/",
      "/.vscode/",
      "/.history/",
      "/node_modules/"
    ],
    "testMatch": ["**/__tests__/*.(ts|tsx|js)"]
  },
  "_moduleAliases": {
    "models": "src/models",
    "utils": "src/utils",
    "images": "src/images/*",
    "components": "src/components/*",
    "src": "src"
  }
}
